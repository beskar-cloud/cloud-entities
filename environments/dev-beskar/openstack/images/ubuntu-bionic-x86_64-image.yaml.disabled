name: ubuntu-bionic-x86_64
image_source_url: https://cloud-images.ubuntu.com/bionic/current/bionic-server-cloudimg-amd64.img
image_source_checksum_url: https://cloud-images.ubuntu.com/bionic/current/SHA256SUMS
image_source_checksum_type: sha256
container_format: bare
disk_format: raw
visibility: public
min_disk: 3
min_ram: 1024
properties:
  min_disk_gb:         "3"
  min_ram_mb:          "1024"
  requires_rdp:        "False"
  requires_ssh:        "True"
  os_type:             linux
  os_distro:           ubuntu
  os_flavor:           Ubuntu
  os_version:          "18.04.6"
  default_user:        ubuntu
  hw_scsi_model:       virtio-scsi
  hw_disk_bus:         scsi
  hw_rng_model:        virtio
  hw_qemu_guest_agent: "yes"
  os_require_quiesce:  "yes"
