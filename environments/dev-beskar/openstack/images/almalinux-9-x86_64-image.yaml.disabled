name: almalinux-9-x86_64
image_source_url: https://repo.almalinux.org/almalinux/9/cloud/x86_64/images/AlmaLinux-9-GenericCloud-latest.x86_64.qcow2
image_source_checksum_url: https://repo.almalinux.org/almalinux/9/cloud/x86_64/images/CHECKSUM
image_source_checksum_type: sha256
container_format: bare
disk_format: raw
visibility: public
min_disk: 8
min_ram: 1024
properties:
  min_disk_gb:         "8"
  min_ram_mb:          "1024"
  requires_rdp:        "False"
  requires_ssh:        "True"
  os_type:             linux
  os_distro:           almalinux
  os_flavor:           AlmaLinux
  os_version:          "9.3"
  default_user:        almalinux
  hw_scsi_model:       virtio-scsi
  hw_disk_bus:         scsi
  hw_rng_model:        virtio
  hw_qemu_guest_agent: "yes"
  os_require_quiesce:  "yes"
