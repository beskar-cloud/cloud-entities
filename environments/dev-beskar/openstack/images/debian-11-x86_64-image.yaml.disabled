name: debian-11-x86_64
image_source_url: https://cloud.debian.org/images/cloud/bullseye/latest/debian-11-genericcloud-amd64.qcow2
image_source_checksum_url: https://cloud.debian.org/images/cloud/bullseye/latest/SHA512SUMS
image_source_checksum_type: sha512
container_format: bare
disk_format: raw
visibility: public
min_disk: 3
min_ram: 1024
properties:
  min_disk_gb:         "3"
  min_ram_mb:          "1024"
  requires_rdp:        "False"
  requires_ssh:        "True"
  os_type:             linux
  os_distro:           debian
  os_flavor:           Debian
  os_version:          "10.11"
  default_user:        debian
  hw_scsi_model:       virtio-scsi
  hw_disk_bus:         scsi
  hw_rng_model:        virtio
  hw_qemu_guest_agent: "yes"
  os_require_quiesce:  "yes"
