name: debian-12-x86_64
image_source_url: https://cloud.debian.org/images/cloud/bookworm/latest/debian-12-genericcloud-amd64.qcow2
image_source_checksum_url: https://cloud.debian.org/images/cloud/bookworm/latest/SHA512SUMS
image_source_checksum_type: sha512
container_format: bare
disk_format: raw
visibility: public
min_disk: 8
min_ram: 2048
properties:
  min_disk_gb:         "8"
  min_ram_mb:          "2048"
  requires_rdp:        "False"
  requires_ssh:        "True"
  os_type:             linux
  os_distro:           debian
  os_flavor:           Debian
  os_version:          "12.5"
  default_user:        debian
  hw_scsi_model:       virtio-scsi
  hw_disk_bus:         scsi
  hw_rng_model:        virtio
  hw_qemu_guest_agent: "yes"
  os_require_quiesce:  "yes"
