name: rockylinux-8-x86_64
image_source_url: https://download.rockylinux.org/pub/rocky/8/images/x86_64/Rocky-8-GenericCloud.latest.x86_64.qcow2
image_source_checksum_url: https://download.rockylinux.org/pub/rocky/8/images/x86_64/Rocky-8-GenericCloud.latest.x86_64.qcow2.CHECKSUM
image_source_checksum_type: sha256
container_format: bare
disk_format: raw
visibility: public
min_disk: 8
min_ram: 1024
properties:
  min_disk_gb:         "8"
  min_ram_mb:          "1024"
  requires_rdp:        "False"
  requires_ssh:        "True"
  os_type:             linux
  os_distro:           rockylinux
  os_flavor:           RockyLinux
  os_version:          "8.9"
  default_user:        rocky
  hw_scsi_model:       virtio-scsi
  hw_disk_bus:         scsi
  hw_rng_model:        virtio
  hw_qemu_guest_agent: "yes"
  os_require_quiesce:  "yes"
